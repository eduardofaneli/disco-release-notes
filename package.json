{
  "name": "disco-release-notes",
  "version": "1.0.0",
  "type": "module",
  "description": "Aplicação web para converter Markdown em mensagens Discord - Release Notes",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite",
    "build": "tsc && vite build",
    "build:github": "cross-env GITHUB_PAGES=true tsc && vite build",
    "build:gh-pages": "tsc -b && vite build --mode production",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "install:all": "pnpm install",
    "clean": "rimraf dist node_modules/.cache",
    "type-check": "tsc --noEmit",
    "setup": "pwsh ./setup-pnpm.ps1",    
    "postinstall": "echo 'Dependências instaladas com pnpm! Use: pnpm dev para iniciar'"
  },
  "packageManager": "pnpm@8.15.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eduardofaneli/disco-release-notes.git"
  },
  "keywords": [
    "discord",
    "markdown",
    "release-notes",
    "converter",
    "webhooks"
  ],
  "author": "Eduardo Faneli <eduardofaneli2@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eduardofaneli/disco-release-notes/issues"
  },
  "homepage": "https://github.com/eduardofaneli/disco-release-notes#readme",
  "pnpm": {
    "overrides": {
      "@types/react": "^18.2.43",
      "@types/react-dom": "^18.2.17"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-dom"
      ]
    }
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "fs-extra": "^11.2.0",
    "helmet": "^7.1.0",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.294.0",
    "only-allow": "^1.2.1",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.20.1",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.3.6",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.19.4",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
